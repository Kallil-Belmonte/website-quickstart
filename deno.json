{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite --host",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@1.0.1",
    "oxlint": "npm:oxlint@0.14.0",
    "sass-embedded": "npm:sass-embedded@1.81.0",
    "typescript": "npm:typescript@5.7.2",
    "vite": "npm:vite@6.0.1",
    "vite-plugin-oxlint": "npm:vite-plugin-oxlint@1.1.0"
  }
}
