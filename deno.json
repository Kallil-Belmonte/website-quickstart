		"sass-embedded": "npm:sass-embedded@1.80.3",
		"typescript": "npm:typescript@5.6.3",
		"vite": "npm:vite@5.4.10",
		"vite-plugin-biome": "npm:vite-plugin-biome@1.0.12"
	}
